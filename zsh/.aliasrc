#!/bin/zsh

alias brewclean="brew cleanup --prune=all"
alias h="hexyl"
alias icat="kitten icat"
alias m="neomutt"
alias n="fastfetch"
alias rebuild-darwin="darwin-rebuild switch --flake ~/.config/nix-darwin"
alias s="kitten ssh"

alias fdown="ffsend download"
alias fh="ffsend history"
alias frm="ffsend delete"
alias fup="ffsend upload -c -P"

alias ga="git add"
alias gd="git diff"
alias gds="git diff --staged"
alias gl="git logs"
alias gla="git last"
alias gp="git push"
alias gr="git restore"
alias grs="git restore --staged"
alias gs="git status"
alias gsh="git show"
alias gv="git vcommit"
